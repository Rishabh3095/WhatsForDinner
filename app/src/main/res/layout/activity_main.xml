<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/constraint"
    tools:context="com.example.rishabh.whatsfordinner.MainActivity">

    <ImageButton
        android:id="@+id/WFD"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/wfd"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="16dp"
        app:layout_constraintVertical_bias="0.01" />

    <ImageButton
        android:id="@+id/meal"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:scaleType="fitCenter"
        app:srcCompat="@drawable/meal"
        app:layout_constraintRight_toLeftOf="@+id/add"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/groc"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginTop="4dp"
        app:layout_constraintTop_toBottomOf="@+id/WFD"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="12dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintVertical_bias="0.702" />

    <ImageButton
        android:id="@+id/recipe"
        android:layout_width="0dp"
        android:layout_height="118dp"
        android:scaleType="fitCenter"
        app:srcCompat="@drawable/recipe"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/add"
        android:layout_marginStart="11dp"
        android:layout_marginEnd="15dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/WFD"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="20dp"
        app:layout_constraintLeft_toRightOf="@+id/meal"
        android:layout_marginLeft="11dp"
        android:layout_marginRight="15dp"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintVertical_bias="0.0" />

    <ImageButton
        android:id="@+id/groc"
        android:layout_width="0dp"
        android:layout_height="88dp"
        android:scaleType="fitCenter"
        app:srcCompat="@drawable/groceries"
        app:layout_constraintRight_toLeftOf="@+id/recipe"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginEnd="11dp"
        android:layout_marginTop="28dp"
        app:layout_constraintTop_toBottomOf="@+id/recipe"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="30dp"
        app:layout_constraintLeft_toLeftOf="@+id/meal"
        android:layout_marginRight="11dp"
        app:layout_constraintHorizontal_bias="0.642"
        app:layout_constraintVertical_bias="0.529"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp" />

    <ImageButton
        android:id="@+id/add"
        android:layout_width="170dp"
        android:layout_height="78dp"
        android:scaleType="fitCenter"
        app:srcCompat="@drawable/add"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginEnd="29dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginBottom="30dp"
        android:layout_marginRight="29dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="MEAL"
        tools:layout_constraintTop_creator="1"
        android:layout_marginStart="37dp"
        app:layout_constraintTop_toBottomOf="@+id/meal"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/meal"
        android:layout_marginLeft="37dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Recipe"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        android:layout_marginStart="68dp"
        android:layout_marginEnd="65dp"
        app:layout_constraintRight_toRightOf="@+id/add"
        app:layout_constraintTop_toBottomOf="@+id/recipe"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/recipe"
        android:layout_marginLeft="68dp"
        android:layout_marginRight="65dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Groceries"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginEnd="28dp"
        app:layout_constraintRight_toRightOf="@+id/groc"
        android:layout_marginBottom="3dp"
        android:layout_marginRight="28dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Add Recipe"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginEnd="38dp"
        app:layout_constraintRight_toRightOf="@+id/add"
        android:layout_marginBottom="3dp"
        android:layout_marginRight="38dp" />

</android.support.constraint.ConstraintLayout>
